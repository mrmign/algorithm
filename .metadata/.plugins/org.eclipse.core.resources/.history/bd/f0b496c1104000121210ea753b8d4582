/*
 * dir
 *
 * |0 |1|
 * |-1|*|
 */
typedef struct st
{
	int cnt; // count
	int dir; //driction
} data_t;

void lcs_length(int *, int *);
void print_lcs(int *, int, int);

#define SIZE 1000
data_t c[SIZE + 1][SIZE + 1];
//int a[SIZE+1];
//int b[SIZE+1];
int main(int argc, char **argv)
{

	FILE *fin = fopen("1000.txt", "r");
	int a[SIZE + 1], b[SIZE + 1];

	int i = 1;
	while (i<=SIZE)
	{
		fscanf(fin,"%d",&a[i]);
		b[i] = a[i];
		i++;
	}
	return 0;
}

void lcs_length(int *x, int *y)
{
	int i, j;
	for (i = 1; i <= SIZE; ++i)
		for (j = 1; j <= SIZE; j++)
		{
			if (x[i] == y[j])
			{
				c[i][j].cnt = c[i - 1][j - 1].cnt + 1;
				c[i][j].dir = 0;
			}
			else if (c[i - 1][j].cnt >= c[i][j - 1].cnt)
			{
				c[i][j].cnt = c[i - 1][j].cnt;
				c[i][j].dir = 1;
			}
			else
			{
				c[i][j].cnt = c[i][j - 1].cnt;
				c[i][j].dir = -1;
			}
		}
}

void print_lcs(int *a, int i, int j)
{
	if (i == 0 || j == 0)
		return;
	if (c[i][j].dir == 0)
	{
		print_lcs(a, i - 1, j - 1);
		printf("%d\n", a[i]);
	}
	else if (c[i][j].dir == 1)
		print_lcs(a, i - 1, j);
	else
	{
		print_lcs(a, i, j - 1);
	}
}
